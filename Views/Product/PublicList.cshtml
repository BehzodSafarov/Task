@model List<Product>

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Product List</h2>
		</div>
	</div>
    <br />
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    #
                </th>
                <th>
                    Title
                </th>
                
                <th>
                    Quontity
                </th>
                <th>
                    Price
                </th>
                
            </tr>
        </thead>
        <tbody>
            @foreach(var obj in Model)
            {
                <tr>
                   <td width="20%">
                        @obj.Id
                    </td>
                    <td width="20%">
                        @obj.Title
                    </td>
                    <td width="20%">
                        @obj.Quantity
                    </td>
                    <td width="20%">
                        @obj.Price
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>